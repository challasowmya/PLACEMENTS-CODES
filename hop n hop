import java.io.*; import java.util.*;

public class Solution {

public static void main(String[] args) {
    Scanner sc = new Scanner(System.in);
    int x1 =3;
    int y1 =4;
    int x = sc.nextInt();
    int y = sc.nextInt();
    int a = (x-x1)*(x-x1);
    int b = (y-y1)*(y-y1);
    double c = Math.sqrt(a+b);
    System.out.print(Math.round(c));

}
}
